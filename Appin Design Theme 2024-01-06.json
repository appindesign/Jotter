{
    "name": "Appin Design Template",
    "dataColors": [
        "#CCCCCC",
        "#4CA948",
        "#05549C",
        "#DAE9EC",
        "#82AACE",
        "#F6F8F9",
        "#292E6B",
        "#E6ECF0"
    ],
    "textClasses": {
        "title": {
            "fontFace": "Verdana"
        },
        "header": {
            "fontFace": "Verdana"
        },
        "label": {
            "fontFace": "Verdana"
        },
        "callout": {
            "fontFace": "Verdana"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "radius": 7,
                        "strokeWidth": 1,
                        "responsive": false,
                        "gridlineShow": false,
                        "showAxisTitle": false,
                        "labelDisplayUnits": "none",
                        "legend": {
                            "show": false,
                            "titleText": "",
                            "position": "TopRight"
                        },
                        "visualHeader": [
                            {
                                "show": true
                            }
                        ],
                        "stylePreset": "Minimal",
                        "title": [
                            {
                                "show": true,
                                "fontColor": {
                                    "solid": {
                                        "color": {
                                            "expr": {
                                                "ThemeDataColor": {
                                                    "ColorId": 1,
                                                    "Percent": 0
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        ],
                        "grid": [
                            {
                                "outlineWeight": 1,
                                "outlineColor": {
                                    "solid": {
                                        "color": {
                                            "expr": {
                                                "ThemeDataColor": {
                                                    "ColorId": 1,
                                                    "Percent": 0.6
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        ],
                        "labelColor": {
                            "solid": {
                                "color": {
                                    "expr": {
                                        "ThemeDataColor": {
                                            "ColorId": 1,
                                            "Percent": 0.6
                                        }
                                    }
                                }
                            }
                        },
                        "secLabelColor": {
                            "solid": {
                                "color": {
                                    "expr": {
                                        "ThemeDataColor": {
                                            "ColorId": 1,
                                            "Percent": 0.6
                                        }
                                    }
                                }
                            }
                        },
                        "border": [
                            {
                                "show": true,
                                "color": {
                                    "solid": {
                                        "color": "#00ff00"
                                    }
                                }
                            }
                        ],
                        "dropShadow": [
                            {
                                "show": true,
                                "color": {
                                    "solid": {
                                        "color": "#ff0000"
                                    }
                                },
                                "position": "Outer",
                                "preset": "TopLeft"
                            }
                        ]
                    }
                ]
            }
        },
        "card": {
            "*": {
                "labels": [
                    {
                        "fontSize": 16
                    }
                ],
                "categoryLabels": [
                    {
                        "show": false,
                        "fontSize": 12
                    }
                ]
            }
        },
        "pivotTable": {
            "*": {
                "subTotals": [
                    {
                        "rowSubTotals": false,
                        "columnSubTotals": false
                    }
                ]
            }
        },
        "map": {
            "*": {
                "mapStyles": [
                    {
                        "mapTheme": "aerial"
                    }
                ]
            }
        }
    }
}